<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox id="AnchorPane" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.softhaxi.insofth.controller.ActivityController">
    <children>
        <Label layoutX="14.0" layoutY="12.0" text="%activities.title" textFill="#ff6600">
            <font>
                <Font size="18.0" />
            </font>
            <VBox.margin>
                <Insets bottom="10.0" />
            </VBox.margin>
        </Label>
                    
        <TableView prefHeight="220.0" prefWidth="580.0" BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn prefWidth="100.0" text="Date" />
                <TableColumn prefWidth="155.0" text="Description" />
                <TableColumn prefWidth="38.0" text="Category" />
                <TableColumn prefWidth="75.0" text="Payment Method" />
                <TableColumn prefWidth="75.0" text="Amount" />
                <TableColumn prefWidth="75.0" text="Collaboration" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
    </padding>
</VBox>