<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="newIncomePane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.softhaxi.insofth.controller.NewIncomeController">
    <stylesheets>
        <URL value="@/styles/Styles.css" />
    </stylesheets>
   <padding>
      <Insets top="5.0" />
   </padding>
   <children>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label prefWidth="100.0" text="%date.title" textFill="#ff6600" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="dtpDate" editable="false" promptText="%date.title" HBox.hgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label prefWidth="100.0" text="%account.title" textFill="#ff6600" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="txtAccount" editable="false" promptText="%account.title" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" HBox.hgrow="ALWAYS">
               <graphic>
                  <ImageView preserveRatio="true" smooth="true">
                     <image>
                        <Image url="@../images/ic_search.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label prefWidth="100.0" text="%amount.titile" textFill="#ff6600" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="txtAmount" alignment="CENTER_RIGHT" promptText="0" text="0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" HBox.hgrow="ALWAYS">
               <graphic>
                  <ImageView preserveRatio="true" smooth="true">
                     <image>
                        <Image url="@../images/ic_calc.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label prefWidth="100.0" text="%payer.title" textFill="#ff6600" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="txtPayer" editable="false" promptText="%payer.title" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" HBox.hgrow="ALWAYS">
               <graphic>
                  <ImageView preserveRatio="true" smooth="true">
                     <image>
                        <Image url="@../images/ic_search.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label prefWidth="100.0" text="%category.title" textFill="#ff6600" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="txtCategory" editable="false" promptText="%category.title" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" HBox.hgrow="ALWAYS">
               <graphic>
                  <ImageView preserveRatio="true" smooth="true">
                     <image>
                        <Image url="@../images/ic_search.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label prefWidth="100.0" text="%payment.title" textFill="#ff6600" wrapText="true" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="txtPayment" editable="false" promptText="%payment.title" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" HBox.hgrow="ALWAYS">
               <graphic>
                  <ImageView preserveRatio="true" smooth="true">
                     <image>
                        <Image url="@../images/ic_search.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <VBox alignment="CENTER_LEFT">
         <children>
            <Label text="Description" textFill="#ff6600">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <TextArea fx:id="txtDescription" prefHeight="80.0" promptText="Description about income" wrapText="true" />
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </VBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label prefWidth="100.0" text="%tax.title" textFill="#ff6600" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="txtTax" editable="false" promptText="%tax.title" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false">
               <graphic>
                  <ImageView preserveRatio="true" smooth="true">
                     <image>
                        <Image url="@../images/ic_calc.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
      <HBox prefHeight="40.0">
         <children>
            <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnSave" disable="true" mnemonicParsing="false" text="Save">
               <HBox.margin>
                  <Insets left="4.0" right="4.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="Cancel">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="4.0" left="10.0" right="10.0" top="4.0" />
         </padding>
      </HBox>
      
   </children>
</VBox>
